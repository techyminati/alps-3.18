		};

		M4U@0x10205000 {
			cell-index = <0>;
			compatible = "mediatek,M4U";
			reg = <0x10205000 0x1000>;
			interrupts = <0 146 0x8>;
			clocks = <&infrasys INFRA_M4U>,
